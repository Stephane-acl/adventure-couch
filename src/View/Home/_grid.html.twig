    {% include "Home/_modal_question.html.twig" %}
    {% set nbCol = 6 %}
    {% set nbLine = 6 %}
    {% set couch = "x-" ~ couchPosition.abs ~ "_y-" ~ couchPosition.ord %}
    {% set tree = 'x-2_y-3' %}
    {% set plane =  'x-4_y-5' %}
    {% set mountain =  'x-1_y-4' %}
    {% set Alpes1 =  'x-6_y-4' %}
    {% set Alpes2 =  'x-6_y-5' %}
    {% set Alpes3 =  'x-5_y-3' %}
    {% set Alpes4 =  'x-6_y-3' %}
    {% set Alpes5 =  'x-6_y-2' %}
    {% set tv =  'x-6_y-1' %}
    {% set iconFive =  'x-1_y-1' %}
    {% set iconTwo =  'x-4_y-5' %}
    {% set iconThree =  'x-1_y-4' %}
    {% set iconFour =  'x-6_y-1' %}
    {% set question =  'x-6_y-5' %}

    {# pour chaque ligne #}
    {% for i in nbLine..1 %}
        <div class="container-fluid col-12 ">
            {# formatage de la  ligne en row de 12 colones #}
            <div class="row justify-content-center mx-0 flex-nowrap">
                {# construction des colones#}
                {% for j in 1..nbCol %}
                    {# variable pour l'id des box ( div ) --> correspond aux coordonées codées sur la grille de type x et y #}
                    {% set box= "x-" ~ j ~ "_" ~ "y-" ~ i %}
                    {% if couch == box %}
                        <div id="{{ box }}" class="col-2 img-content px-0 gc-caseSize gc-boxIcon">
                            <div id="couch"><img class="icon" id="couchColor" src="/assets/images/couch-solid.svg">
                            </div>
                        </div>

                        {# detection si id box en cour = id iconOne #}
                    {% elseif tree == box %}
                        <div id="{{ box }}" class="col-2 img-content px-0 gc-caseSize gc-boxIcon gc-icon1">
                            {# put icon 1 in div #}
                            <div> {% include "Home/_tree-solid.html.twig" %} </div>
                        </div>
                        {# detection si id box en cour = id iconTwo #}
                    {% elseif plane == box %}
                        <div id="{{ box }}" class="col-2 img-content px-0 gc-caseSize gc-icon2 gc-boxIcon">
                            {# put icon 2 in div #}
                            <div>  {% include "Home/_plane-departure-solid.html.twig" %}  </div>
                        </div>
                        {# detection si id box en cour = id iconThree #}
                    {% elseif mountain == box %}
                        <div id="{{ box }}" class="col-2 img-content px-0 gc-caseSize gc-icon3 gc-boxIcon">
                            {# put icon 3 in div #}
                            <div class="mountain">  {% include "Home/_mountain-solid.html.twig" %}  </div>
                        </div>
                    {% elseif Alpes == box %}
                        <div id="{{ box }}"
                             class="mountain alpes col-2 img-content px-0 gc-caseSize gc-icon3 gc-boxIcon">
                            {# put icon 3 in div #}
                            <div>  {% include "Home/_mountain-solid.html.twig" %}  </div>
                        </div>
                    {% elseif Alpes1 == box %}
                        <div id="{{ box }}"
                             class="mountain alpes col-2 img-content px-0 gc-caseSize gc-icon3 gc-boxIcon">
                            {# put icon 3 in div #}
                            <div>  {% include "Home/_mountain-solid.html.twig" %}  </div>
                        </div>
                    {% elseif Alpes2 == box %}
                        <div id="{{ box }}"
                             class="mountain alpes col-2 img-content px-0 gc-caseSize gc-icon3 gc-boxIcon">
                            {# put icon 3 in div #}
                            <div>  {% include "Home/_mountain-solid.html.twig" %}  </div>
                        </div>
                    {% elseif Alpes3 == box %}
                        <div id="{{ box }}"
                             class="mountain alpes col-2 img-content px-0 gc-caseSize gc-icon3 gc-boxIcon">
                            {# put icon 3 in div #}
                            <div>  {% include "Home/_mountain-solid.html.twig" %}  </div>
                        </div>
                    {% elseif Alpes4 == box %}
                        <div id="{{ box }}"
                             class="mountain alpes col-2 img-content px-0 gc-caseSize gc-icon3 gc-boxIcon">
                            {# put icon 3 in div #}
                            <div>  {% include "Home/_mountain-solid.html.twig" %}  </div>
                        </div>
                    {% elseif Alpes5 == box %}
                        <div id="{{ box }}"
                             class="mountain alpes col-2 img-content px-0 gc-caseSize gc-icon3 gc-boxIcon">
                            {# put icon 3 in div #}
                            <div>  {% include "Home/_mountain-solid.html.twig" %}  </div>
                        </div>


                        {# detection si id box en cour = id iconFour #}
                    {% elseif tv == box %}
                        <div id="{{ box }}" class="col-2 img-content px-0 gc-caseSize gc-icon4 gc-boxIcon">
                            {# put icon 4 in div #}
                            <div>  {% include "Home/_tv-solid.html.twig" %}  </div>
                        </div>
                    {% elseif question == box %}
                        <div id="{{ box }}" class="col-2 img-content px-0 gc-caseSize gc-icon4 gc-boxIcon">
                            {# put icon 5 in div #}
                            <div>  {% include "Home/_question-circle-regular.html.twig" %}  </div>
                        </div>
                        {# si aucun cas ne corespond affichage d'une box vide #}
                    {% else %}
                        <div id="{{ box }}" class="col-2 img-content px-0 gc-caseSize">
                            <div> {{ idBox }} </div>
                        </div>
                    {% endif %}
                {% endfor %}
            </div>
        </div>
    {% endfor %}


